config wifi-device 'wifi1'
	option type 'qcawifi'
	option channel 'auto'
	option phy 'phy0'
	option hwmode '11ac'
	option txpower '0'
	option fasthandover_status '0'
	option fasthandover_rssi '-70'
	option htmode 'HT80'
	option opmode 'ap'
	option disabled '0'
	option country '00'
	option clientlimits_enable '0'
	option clientlimits_number '127'
	option aggregation_enable '1'
	option aggregation_frame '32'
	option aggregation_byte '50000'
	option obeyregpower 'undefined'

config wifi-device 'wifi0'
	option type 'qcawifi'
	option channel 'auto'
	option phy 'phy0'
	option hwmode '11ng'
	option txpower '0'
	option bandsteer '0'
	option fasthandover_status '0'
	option fasthandover_rssi '-70'
	option htmode 'HT20_40'
	option rate '0x0'
	option opmode 'ap'
	option disabled '0'
	option country '00'
	option clientlimits_enable '0'
	option clientlimits_number '127'
	option aggregation_enable '1'
	option aggregation_frame '32'
	option aggregation_byte '50000'
	option obeyregpower 'undefined'

config wifi-iface
	option device 'wifi0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Sitecom-mac-_1-2.4GHz'
	option encryption 'none'
	option bintval '400'
	option dtim_period '2'
	option frag '2346'
	option rts '2346'
	option preamble '0'
	option nawds '0'
	option wds '0'
	option disabled '0'
	option mode_display 'ap'
	option ifname 'ath0'
	option hidden '0'
	option isolate '0'
	option isolation '0'
	option vlan_id '1'
	option tc_enabled '0'
	option index '0'
	
